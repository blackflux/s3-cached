{
  "name": "s3-cached",
  "version": "0.0.0-development",
  "description": "S3 File Access Abstraction providing Memory and Disk Caching Layer",
  "main": "lib/s3-cached.js",
  "scripts": {
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "npm run build-artifact && npm run clean-build && npx semantic-release",
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --source-maps inline --copy-files --include-dotfiles",
    "clean-build": "npm run clean && npm run build",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "gardener": "node gardener",
    "build-artifact": "npm run clean-build && tar -zxvf $(npm pack) && rm *-0.0.0-development.tgz && cd package && tar -zcvf ../release.tar.gz . && cd .. && rm -rf package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simlu/s3-cached.git"
  },
  "keywords": [
    "s3",
    "aws",
    "cache",
    "memory-cache",
    "disk-cache",
    "files",
    "storage",
    "cloud"
  ],
  "author": "Lukas Siemon",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/simlu/s3-cached/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/simlu/s3-cached/issues"
  },
  "homepage": "https://github.com/simlu/s3-cached#readme",
  "dependencies": {
    "aws-sdk": "2.287.0",
    "lodash.defaults": "4.2.0",
    "cache-manager": "2.9.0",
    "cache-manager-fs": "1.0.6"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "nock": "9.4.3",
    "lodash.get": "4.4.2",
    "js-gardener": "1.30.3",
    "coveralls": "3.0.2",
    "mocha": "5.2.0",
    "semantic-release": "15.8.1",
    "nyc": "12.0.2",
    "babel-cli": "7.0.0-beta.3",
    "babel-register": "7.0.0-beta.3",
    "babel-eslint": "8.2.6",
    "babel-plugin-transform-flow-comments": "7.0.0-beta.3",
    "babel-preset-flow": "7.0.0-beta.3"
  },
  "engines": {
    "node": ">= 6.10.3"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {},
  "nyc": {
    "tempDirectory": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "flow-typed/*",
      "lib/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true
  }
}
